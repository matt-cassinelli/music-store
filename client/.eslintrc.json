{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "next",
        "next/core-web-vitals",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["warn", "double"],
        "prefer-const": "warn",
        "semi": "warn",
        "indent": ["warn", 2],
        "no-unused-vars": "warn",
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off"
    }
}
